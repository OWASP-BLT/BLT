{
  "name": "bug-reporter",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "wrangler pages deploy dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint --ext .ts,.tsx src --max-warnings=0",
